{
    // Detailed configuration descriptions: see https://github.com/GaiZhenbiao/ChuanhuChatGPT/wiki/%E4%BD%BF%E7%94%A8%E6%95%99%E7%A8%8B#%E9%85%8D%E7%BD%AE-configjson

    //== API settings ==
    "openai_api_key": "", // Your OpenAI API key. Usually required; if blank, enter it in the UI
    "deepseek_api_key": "", // Your DeepSeek API key for DeepSeek Chat and Reasoner(R1)
    "google_genai_api_key": "", // Your Google Gemini API key for Gemini models
    "google_genai_api_host": "generativelanguage.googleapis.com", // Your Google Gemini API host; usually unchanged
    "xmchat_api_key": "", // Your xmchat API key for XMChat models
    "minimax_api_key": "", // Your MiniMax API key for MiniMax models
    "minimax_group_id": "", // Your MiniMax Group ID for MiniMax models
    "midjourney_proxy_api_base": "https://xxx/mj", // Your https://github.com/novicezk/midjourney-proxy proxy URL
    "midjourney_proxy_api_secret": "", // Your MidJourney Proxy API secret for auth (optional)
    "midjourney_discord_proxy_url": "", // Your MidJourney Discord proxy URL for image proxying (optional)
    "midjourney_temp_folder": "./tmp", // MidJourney temp folder for generated images; empty to disable auto downloads and show four-grid images
    "spark_appid": "", // Your iFlytek Spark API AppID for Spark models
    "spark_api_key": "", // Your iFlytek Spark API key for Spark models
    "spark_api_secret": "", // Your iFlytek Spark API secret for Spark models
    "claude_api_secret":"",// Your Claude API secret for Claude models
    "ernie_api_key": "",// Your ERNIE Bot API key on Baidu Cloud for ERNIE models
    "ernie_secret_key": "",// Your ERNIE Bot secret key on Baidu Cloud for ERNIE models
    "ollama_host": "", // Your Ollama host for Ollama models
    "huggingface_auth_token": "", // Your Hugging Face API token for restricted models
    "groq_api_key": "", // Your Groq API key for Groq models (https://console.groq.com/)

    //== Azure ==
    "openai_api_type": "openai", // Options: azure, openai
    "azure_openai_api_key": "", // Your Azure OpenAI API key for Azure OpenAI models
    "azure_openai_api_base_url": "", // Your Azure base URL
    "azure_openai_api_version": "2023-05-15", // Your Azure OpenAI API version
    "azure_deployment_name": "", // Your Azure OpenAI Chat deployment name
    "azure_embedding_deployment_name": "", // Your Azure OpenAI Embedding deployment name
    "azure_embedding_model_name": "text-embedding-ada-002", // Your Azure OpenAI Embedding model name

    //== Basic settings ==
    "language": "auto", // UI language; options: auto, zh_CN, en_US, ja_JP, ko_KR, sv_SE, ru_RU, vi_VN
    "users": [], // User list, [["user1", "pass1"], ["user2", "pass2"], ...]
    "admin_list": [], // Admin list, ["user1", "user2", ...] Only admins can restart the service
    "local_embedding": false, // Whether to index locally
    "hide_history_when_not_logged_in": false, // Hide chat history when not logged in
    "check_update": true, // Enable update checks
    "default_model": "GPT3.5 Turbo", // Default model
    "chat_name_method_index": 2, // Chat naming method: 0=timestamp, 1=first question, 2=model summary
    "bot_avatar": "default", // Bot avatar: local path, URL, or "none" to hide
    "user_avatar": "default", // User avatar: local path, URL, or "none" to hide

    //== API usage ==
    "show_api_billing": false, // Show OpenAI usage (requires sensitive_id)
    "sensitive_id": "", // Your OpenAI account sensitive ID for usage queries
    "usage_limit": 120, // Monthly limit for this OpenAI key in USD for percentage calculations
    "legacy_api_usage": false, // Use legacy usage API (OpenAI deprecated; some third-party APIs may still support)

    //== Chuanhu Assistant settings ==
    "GOOGLE_CSE_ID": "", // Google Search Engine ID for Chuanhu Assistant Pro; see https://stackoverflow.com/questions/37083058/programmatically-searching-google-in-python-using-custom-search
    "GOOGLE_API_KEY": "", // Google API key for Chuanhu Assistant Pro
    "WOLFRAM_ALPHA_APPID": "", // Wolfram Alpha API key for Chuanhu Assistant Pro; see https://products.wolframalpha.com/api/
    "SERPAPI_API_KEY": "", // SerpAPI API key for Chuanhu Assistant Pro; see https://serpapi.com/

    //== Document processing and display ==
    "latex_option": "default", // LaTeX rendering strategy: default, strict, all, or disabled
    "advance_docs": {
        "pdf": {
            "two_column": false, // Assume PDFs are double-column
            "formula_ocr": true // Use OCR to recognize formulas in PDFs
        }
    },

    //== Advanced settings ==
    // Rotate multiple API keys
    "multi_api_key": false,
    "hide_my_key": false, // Set true to hide the API key field in the UI
    // "available_models": ["GPT3.5 Turbo", "GPT4 Turbo", "GPT4 Vision"], // Available model list; overrides defaults
